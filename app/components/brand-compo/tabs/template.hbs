          <li>{{link-to "PRODUCTS" "brand.edit.products" session.brand.id class="text-uppercase text-center"}}</li>
        </ul>
      </div>
    </div>
    <div class="mob-nav">
      <button class="btn btn-success dropdown-toggle text-uppercase" type="button" id="brand-settings-menu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
