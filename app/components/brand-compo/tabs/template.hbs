<div class="default-model">
  <div class="modelbox-body">
    <div class="brand_options_tabs_wrapper">
      <div class="brand_options_tabs" id="brand-options-tabs-collapse">
        <ul class="nav nav-tabs tabs-left">
          <li>{{link-to "DETAILS" "brand.edit.details" session.brand.id class="text-uppercase text-center"}}</li>
          <li>{{link-to "SOCIAL ACCOUNTS" "brand.edit.socialaccounts" session.brand.id class="text-uppercase text-center"}}</li>
