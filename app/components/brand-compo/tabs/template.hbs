        <li>{{link-to "PRODUCTS" "brand.edit.products" session.brand.id class="text-uppercase text-center"}}</li>
      </ul>
    </div>
    <div class="right-panel">
      <div class="tab-content">
        <div class="tab-pane active" id="home">
          {{yield}}
        </div>
      </div>
    </div>
  </div>
